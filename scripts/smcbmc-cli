#!/usr/bin/env python3
"""smcbmc-cli - Supermicro BMC Management CLI entry point."""

import os
import sys

# Add the scripts directory to the Python path so smcbmc package is importable
script_dir = os.path.dirname(os.path.realpath(__file__))
sys.path.insert(0, script_dir)

from smcbmc.cli import cli

if __name__ == "__main__":
    cli()
